  "nodemonConfig": {
    "quite": true,
    "watch": [
      "src/manifest.js"
    ],
    "ignore": [
      "src/app/*"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,json,css,md}": [
      "prettier --write",
      "git add"
    ]
  }
}
